<Window x:Class="SD_FXUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SD_FXUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="TryPrompt" HorizontalAlignment="Left" Margin="10,19,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="414" Height="54"/>
        <Label Content="Prompt" HorizontalAlignment="Left" Margin="0,-4,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="NegPrompt" HorizontalAlignment="Left" Margin="10,97,0,0" TextWrapping="Wrap" Text="" Width="414" Height="33" VerticalAlignment="Top"/>
        <Button Content="Make!" Margin="12,0,0,151" Click="Button_Click" HorizontalAlignment="Left" Width="37" Height="20" VerticalAlignment="Bottom"/>
        <TextBox x:Name="tbX" HorizontalAlignment="Left" Margin="10,145,0,0" TextWrapping="Wrap" Text="512" VerticalAlignment="Top" Width="33"/>
        <TextBox x:Name="tbH" HorizontalAlignment="Left" Margin="53,145,0,0" TextWrapping="Wrap" Text="512" VerticalAlignment="Top" Width="35"/>
        <Label Content="x" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="40,141,0,0"/>
        <Label Content="Neg prompt" HorizontalAlignment="Left" Margin="0,73,0,0" VerticalAlignment="Top"/>
        <Slider  x:Name="slSteps" HorizontalAlignment="Left" Margin="80,179,0,0" VerticalAlignment="Top" Width="120" Maximum="150" Minimum="5" Value="25" ValueChanged="Slider_ValueChanged"/>
        <Label Content="Steps Count" HorizontalAlignment="Left" Margin="1,175,0,0" VerticalAlignment="Top"/>
        <TextBox  x:Name="tbSteps" HorizontalAlignment="Left" Margin="205,179,0,0" TextWrapping="Wrap" Text="25" VerticalAlignment="Top" Width="34" TextChanged="tbSteps_TextChanged"/>
        <Image x:Name="ViewImg" HorizontalAlignment="Left" Height="298" Margin="461,19,0,0" VerticalAlignment="Top" Width="327"/>
        <Label Content="CFG Scale" HorizontalAlignment="Left" Margin="1,206,0,0" VerticalAlignment="Top"/>
        <Slider  x:Name="slCFG" HorizontalAlignment="Left" Margin="80,210,0,0" VerticalAlignment="Top" Width="120" Maximum="20" Minimum="5" Value="7.5" ValueChanged="Slider2_ValueChanged" SmallChange="0.5"/>
        <TextBox  x:Name="tbCFG" HorizontalAlignment="Left" Margin="205,210,0,0" TextWrapping="Wrap" Text="7,5" VerticalAlignment="Top" Width="34" TextChanged="tbSteps2_TextChanged"/>
        <TextBox x:Name="tbSeed" HorizontalAlignment="Left" Margin="304,145,0,0" TextWrapping="Wrap" Text="-1" VerticalAlignment="Top" Width="120"/>
        <Label Content="Seed" HorizontalAlignment="Left" Margin="263,141,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
