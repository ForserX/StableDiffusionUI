<Window x:Class="SD_FXUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SD_FXUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="TryPrompt" HorizontalAlignment="Left" Margin="10,19,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="414" Height="54"/>
        <Label Content="Prompt" HorizontalAlignment="Left" Margin="0,-4,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="NegPrompt" HorizontalAlignment="Left" Margin="10,97,0,0" TextWrapping="Wrap" Text="" Width="414" Height="39" VerticalAlignment="Top"/>
        <Button Content="Make!" Margin="5,0,737,152" Click="Button_Click" Height="20" VerticalAlignment="Bottom"/>
        <Label Content="x" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="69,141,0,0"/>
        <Label Content="Neg prompt" HorizontalAlignment="Left" Margin="0,73,0,0" VerticalAlignment="Top"/>
        <Slider  x:Name="slSteps" HorizontalAlignment="Left" Margin="80,179,0,0" VerticalAlignment="Top" Width="120" Maximum="150" Minimum="5" Value="25" ValueChanged="Slider_ValueChanged" SmallChange="1"/>
        <Label Content="Steps Count" HorizontalAlignment="Left" Margin="1,175,0,0" VerticalAlignment="Top"/>
        <TextBox  x:Name="tbSteps" HorizontalAlignment="Left" Margin="205,179,0,0" TextWrapping="Wrap" Text="25" VerticalAlignment="Top" Width="34" TextChanged="tbSteps_TextChanged"/>
        <Image x:Name="ViewImg" Margin="456,9,17,127"/>
        <Label Content="CFG Scale" HorizontalAlignment="Left" Margin="1,206,0,0" VerticalAlignment="Top"/>
        <Slider  x:Name="slCFG" HorizontalAlignment="Left" Margin="80,210,0,0" VerticalAlignment="Top" Width="120" Maximum="20" Minimum="5" Value="7.5" ValueChanged="Slider2_ValueChanged" SmallChange="0.5"/>
        <TextBox  x:Name="tbCFG" HorizontalAlignment="Left" Margin="205,210,0,0" TextWrapping="Wrap" Text="7,5" VerticalAlignment="Top" Width="34" TextChanged="tbSteps2_TextChanged"/>
        <TextBox x:Name="tbSeed" HorizontalAlignment="Left" Margin="304,145,0,0" TextWrapping="Wrap" Text="-1" VerticalAlignment="Top" Width="120"/>
        <Label Content="Seed" HorizontalAlignment="Left" Margin="263,141,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbModel" HorizontalAlignment="Left" Margin="5,293,0,0" VerticalAlignment="Top" Width="344">
            <ComboBoxItem Content="Linaqruf/anything-v3.0"/>
            <ComboBoxItem Content="DGSpitzer/Cyberpunk-Anime-Diffusion"/>
            <ComboBoxItem Content="AIAddicts/VashirLewd"/>
            <ComboBoxItem Content="stabilityai/stable-diffusion-2-1"/>
            <ComboBoxItem Content="dreamlike-art/dreamlike-diffusion-1.0"/>
            <ComboBoxItem Content="CompVis/stable-diffusion-v1-4"/>
        </ComboBox>
        <CheckBox x:Name="cbFf16" Content="fp16" HorizontalAlignment="Left" Margin="364,296,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <ComboBox x:Name="cbX" HorizontalAlignment="Left" Margin="10,143,0,0" VerticalAlignment="Top" Width="59">
            <ComboBoxItem Content="256"/>
            <ComboBoxItem Content="384"/>
            <ComboBoxItem Content="512"/>
            <ComboBoxItem Content="1024"/>
        </ComboBox>
        <ComboBox x:Name="cbY" HorizontalAlignment="Left" Margin="85,143,0,0" VerticalAlignment="Top" Width="59">
            <ComboBoxItem Content="256"/>
            <ComboBoxItem Content="384"/>
            <ComboBoxItem Content="512"/>
            <ComboBoxItem Content="1024"/>
        </ComboBox>

    </Grid>
</Window>
