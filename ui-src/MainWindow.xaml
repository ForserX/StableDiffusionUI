<Window x:Class="SD_FXUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SD_FXUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="509" Width="800" Closed="OnClose">
    <Grid Margin="0,0,0,3">
        <TextBox x:Name="TryPrompt" HorizontalAlignment="Left" Margin="10,19,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="414" Height="54"/>
        <Label Content="Prompt" HorizontalAlignment="Left" Margin="0,-4,0,0" VerticalAlignment="Top" Height="26" Width="49"/>
        <TextBox x:Name="NegPrompt" HorizontalAlignment="Left" Margin="10,97,0,0" TextWrapping="Wrap" Text="" Width="414" Height="39" VerticalAlignment="Top"/>
        <Button Content="Make!" Margin="364,0,0,12" Click="Button_Click" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="58"/>
        <Label Content="x" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="117,178,0,0" Height="26" Width="16"/>
        <Label Content="Neg prompt" HorizontalAlignment="Left" Margin="0,73,0,0" VerticalAlignment="Top" Height="26" Width="75"/>
        <Slider  x:Name="slSteps" HorizontalAlignment="Left" Margin="262,149,0,0" VerticalAlignment="Top" Width="120" Maximum="150" Minimum="5" Value="25" ValueChanged="Slider_ValueChanged" IsSnapToTickEnabled="True" TickFrequency="1" Height="18" LargeChange="0"/>
        <Label Content="Steps Count" HorizontalAlignment="Left" Margin="184,145,0,0" VerticalAlignment="Top" Height="26" Width="74"/>
        <TextBox  x:Name="tbSteps" HorizontalAlignment="Left" Margin="388,149,0,0" TextWrapping="Wrap" Text="25" VerticalAlignment="Top" Width="34" TextChanged="tbSteps_TextChanged" Height="18"/>
        <Image x:Name="ViewImg" Margin="429,8,5,3"/>
        <Label Content="CFG Scale" HorizontalAlignment="Left" Margin="184,176,0,0" VerticalAlignment="Top" Height="26" Width="62"/>
        <ComboBox  x:Name="cbDevice" HorizontalAlignment="Left" Margin="77,334,0,0" VerticalAlignment="Top" Width="116" Height="22" Text="vulkan">
            <ComboBoxItem Content="vulkan"/>
            <ComboBoxItem Content="cuda"/>
        </ComboBox>
        <Slider  x:Name="slCFG" HorizontalAlignment="Left" Margin="262,180,0,0" VerticalAlignment="Top" Width="120" Maximum="20" IsSnapToTickEnabled="True" TickFrequency="1" Minimum="5" Value="7.5" ValueChanged="Slider2_ValueChanged" SmallChange="0.5" Height="18"/>
        <TextBox  x:Name="tbCFG" HorizontalAlignment="Left" Margin="388,180,0,0" TextWrapping="Wrap" Text="7,5" VerticalAlignment="Top" Width="34" TextChanged="tbSteps2_TextChanged" Height="18"/>
        <TextBox x:Name="tbSeed" HorizontalAlignment="Left" Margin="69,301,0,0" TextWrapping="Wrap" Text="-1" VerticalAlignment="Top" Width="96" Height="18" IsEnabled="False"/>
        <Label Content="Seed" HorizontalAlignment="Left" Margin="13,297,0,0" VerticalAlignment="Top" Height="26" Width="36"/>
        <ComboBox x:Name="cbModel" HorizontalAlignment="Left" Margin="10,402,0,0" VerticalAlignment="Top" Width="294" Height="22"/>
        <CheckBox x:Name="cbFf16" Content="fp16" HorizontalAlignment="Left" Margin="373,405,0,0" VerticalAlignment="Top" IsChecked="True" Height="15" Width="44"/>
        <ComboBox x:Name="cbX" HorizontalAlignment="Left" Margin="65,180,0,0" VerticalAlignment="Top" Width="52" Height="22">
            <ComboBoxItem Content="384"/>
            <ComboBoxItem Content="416"/>
            <ComboBoxItem Content="512"/>
            <ComboBoxItem Content="640"/>
            <ComboBoxItem Content="728"/>
            <ComboBoxItem Content="1024"/>
        </ComboBox>
        <ComboBox x:Name="cbY" HorizontalAlignment="Left" Margin="132,180,0,0" VerticalAlignment="Top" Width="52" Height="22">
            <ComboBoxItem Content="384"/>
            <ComboBoxItem Content="416"/>
            <ComboBoxItem Content="512"/>
            <ComboBoxItem Content="640"/>
            <ComboBoxItem Content="728"/>
            <ComboBoxItem Content="1024"/>
        </ComboBox>
        <ComboBox x:Name="cbUpscaler" HorizontalAlignment="Left" Margin="77,361,0,0" VerticalAlignment="Top" Width="116" Height="22">
            <ComboBoxItem Content="ERSGAN"/>
            <ComboBoxItem Content="ERSGAN: Anime"/>
            <ComboBoxItem Content="SR"/>
            <ComboBoxItem Content="SRMD"/>
            <ComboBoxItem Content="None"/>
        </ComboBox>

        <ComboBox x:Name="cbSampler" HorizontalAlignment="Left" Margin="65,149,0,0" VerticalAlignment="Top" Width="119" Height="22">
            <ComboBoxItem Content="DDIM"/>
            <ComboBoxItem Content="EulerDiscrete"/>
            <ComboBoxItem Content="EulerAncestralDiscrete"/>
            <ComboBoxItem Content="DPMSolverMultistep"/>
            <ComboBoxItem Content="PNDM"/>
            <ComboBoxItem Content="LMSDiscrete"/>
            <ComboBoxItem Content="SharkEulerDiscrete"/>
            <!--<            
                schedulers["PNDM"] = PNDMScheduler.from_pretrained(       
                schedulers["LMSDiscrete"] = LMSDiscreteScheduler.from_pretrained(       
                schedulers["DDIM"] = DDIMScheduler.from_pretrained(
                schedulers["DPMSolverMultistep"] = DPMSolverMultistepScheduler.from_pretrained(
                schedulers["EulerDiscrete"] = EulerDiscreteScheduler.from_pretrained(
                schedulers["EulerAncestralDiscrete"] = EulerAncestralDiscreteScheduler.from_pretrained(
                schedulers["SharkEulerDiscrete"] = SharkEulerDiscreteScheduler.from_pretrained(
            >-->
            
            
            <!--["pndm", "lms", "heun", "euler","euler-ancestral", "dpm", "ddim"]-->

        </ComboBox>

        <Label Content="Upscale" HorizontalAlignment="Left" Margin="12,359,0,0" VerticalAlignment="Top" Height="26" Width="52" RenderTransformOrigin="0.475,-0.021"/>
        <Label Content="Value" HorizontalAlignment="Left" Margin="211,361,0,0" VerticalAlignment="Top" Height="26" Width="39"/>
        <Slider  x:Name="slUpscale" HorizontalAlignment="Left" Margin="255,365,0,0" VerticalAlignment="Top" Width="58" IsSnapToTickEnabled="True" TickFrequency="1" Maximum="3" Value="3" ValueChanged="slUpscale_ValueChanged" SmallChange="1" Height="18"/>
        <Label x:Name="lbUpscale" Content="x4" HorizontalAlignment="Left" Margin="324,361,0,0" VerticalAlignment="Top" Background="#FFDFD2D2" Height="26" Width="22"/>
        <Image Margin="2,332,0,0" Source="/folder.png" MouseLeftButtonDown="btFolder_ValueChanged" Height="44" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="50"/>
        <TextBox x:Name="tbTotalCount" HorizontalAlignment="Left" Margin="321,301,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="38" Height="18"/>
        <Label Content="Total count" HorizontalAlignment="Left" Margin="251,297,0,0" VerticalAlignment="Top" Height="26" Width="70"/>
        <Image x:Name="btCmd" Margin="42,332,0,0" Source="/cmd.png" MouseLeftButtonDown="btCmd_ValueChanged" Height="44" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="50"/>
        <Button Content="Import" HorizontalAlignment="Left" Margin="316,403,0,0" VerticalAlignment="Top" Width="48" Click="Button_Click_1"/>
        <Button Content="Break!" Margin="295,0,0,12" Click="Button_ClickBreak" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="58" RenderTransformOrigin="0.484,1.102"/>
        <Label Content="Device" HorizontalAlignment="Left" Margin="13,332,0,0" VerticalAlignment="Top" Height="26" Width="49"/>
        <CheckBox x:Name="chRandom" Content="Random" HorizontalAlignment="Left" Margin="175,302,0,0" VerticalAlignment="Top" IsChecked="True" Checked="chRandom_Checked" Unchecked="chRandom_Unchecked"/>
        <Label Content="Img Size" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="4,178,0,0" Height="26" Width="54"/>
        <Label Content="Sampler" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="6,147,0,0" Height="26" Width="54"/>
    </Grid>
</Window>
